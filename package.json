{
  "name": "element-components",
  "version": "1.6.5",
  "author": "Zehui Chan <jasonchenzehui@gmail.com>",
  "main": "dist/ElementComponents.umd.min.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build:github": "vue-cli-service build --mode github",
    "build:gitee": "vue-cli-service build --mode gitee",
    "build": "vue-cli-service build --target lib --name ElementComponents --dest ./dist/ ./lib/index.js",
    "unpublish": "npm unpublish --force",
    "release": "release-it && git push --follow-tags origin main"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:zehuichan/element-components.git"
  },
  "homepage": "https://zehuichan.gitee.io/element-components/#/home",
  "dependencies": {
    "@riophae/vue-treeselect": "^0.4.0",
    "dayjs": "^1.10.6",
    "file-saver": "^2.0.5",
    "jszip": "^3.6.0",
    "lodash.chunk": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "qrcode": "^1.4.4",
    "wangeditor": "^4.7.4",
    "xlsx": "^0.16.9"
  },
  "peerDependencies": {
    "element-ui": "^2.15.3",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@vant/markdown-loader": "^2.5.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "core-js": "^3.15.1",
    "element-ui": "^2.15.3",
    "highlight.js": "^10.7.3",
    "node-sass": "^6.0.1",
    "normalize.css": "^8.0.1",
    "release-it": "^14.10.0",
    "sass-loader": "10.2.0",
    "vue": "^2.6.12",
    "vue-router": "^3.5.2",
    "vue-template-compiler": "^2.6.14"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "push": false,
      "tagName": "v${version}",
      "commitMessage": "chore: release v${version}"
    }
  }
}
