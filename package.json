  "private": true,
  "main": "./dist/@vcomponents/element-ui.common.js",
  "author": "Zehui Chan <jasonchenzehui@gmail.com>",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name @vcomponents/element-ui",
    "publish ": "npm publish ",
